{
  "name": "simple-odk",
  "version": "1.2.1-dev",
  "description": "A minimal ODK server for receiving form submissions from a mobile device and saving them as JSON on github",
  "main": "app.js",
  "engines": {
    "iojs": "~1.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/digidem/simple-odk.git"
  },
  "keywords": [
    "odk",
    "github"
  ],
  "license": "MIT",
  "dependencies": {
    "async": "^0.9.0",
    "basic-auth": "^1.0.0",
    "charset": "^1.0.0",
    "debug": "^2.1.1",
    "express": "^4.12.0",
    "firebase": "^2.2.2",
    "gistfs.js": "0.0.1",
    "hubfs.js": "~0.0.3",
    "knox": "^0.9.2",
    "mime-types": "^2.0.10",
    "mkdirp": "^0.5.0",
    "morgan": "^1.5.2",
    "node-cache": "^1.1.0",
    "octokat": "^0.4.7",
    "openrosa-form-submission-middleware": "^1.0.3",
    "openrosa-formlist": "^1.0.1",
    "openrosa-manifest": "0.0.1",
    "openrosa-request-middleware": "^1.0.1",
    "request": "^2.40.0",
    "resumer": "0.0.0",
    "traverse": "^0.6.6",
    "uid2": "0.0.3",
    "vhost": "^3.0.0",
    "xform-to-json": "^1.1.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "do-wrapper": "^1.0.0",
    "dokku-toolbelt": "0.0.5",
    "dotenv": "^0.5.1",
    "ngrok": "^0.1.99",
    "proxyquire": "^1.4.0",
    "ssh-fingerprint": "0.0.1",
    "standard": "^3.2.1",
    "supertest": "^0.15.0",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0"
  },
  "scripts": {
    "start": "env $(cat .env | xargs) node server.js",
    "test": "standard && tape tests/**/*.test.js | tap-spec",
    "provision": "bin/provision.js",
    "deploy": "bin/deploy.sh"
  },
  "bugs": {
    "url": "https://github.com/digidem/simple-odk/issues"
  },
  "homepage": "https://github.com/digidem/simple-odk",
  "author": "Gregor MacLennan",
  "directories": {
    "test": "tests"
  }
}
